<h1>Create a New Article!!!</h1>
<%= form_for :article, url:articles_path do |f| %>
    <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
    </p>
    <p>
    <%= f.label :text %>
    <%= f.text_area :text %>
    </p>
    <p>
    <%= f.submit %>
    </p>
    </p>
    <% end %>
<p>
    <%= link_to("Back to post list", articles_path) %>
    |
    <%= link_to("Back to home page", welcome_index_path) %>
</p>